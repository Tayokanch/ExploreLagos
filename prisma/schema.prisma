generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")

}

model Location {
  id            Int       @id @default(autoincrement())
  location      String    @unique
  history       String
  price         Int
  category      String
  activities   Json[]
  createdAt     DateTime  @default(now())
  updatedAt     DateTime  @updatedAt

}



